{
  "name": "vite-tauri-template",
  "version": "0.0.1",
  "description": "Vite + Tauri + Vue 3 desktop app template",
  "keywords": [
    "vite",
    "tauri",
    "vue",
    "desktop"
  ],
  "scripts": {
    "lint": "exit 0;",
    "serve": "vite --port 8080",
    "serve:native": "VITE_HTTP_HANDLER=NATIVE vite --port 8080",
    "serve:tauri": "tauri dev",
    "build:web": "vite build",
    "build": "VITE_HTTP_HANDLER=NATIVE vite build && tauri build"
  },
  "dependencies": {
    "@mdi/font": "6.5.95",
    "@tauri-apps/api": "1.0.0-beta.8",
    "roboto-fontface": "0.10.0",
    "vue": "3.2.29",
    "vue-global-api": "0.4.1",
    "vue-router": "4.0.12",
    "ynetwork": "3.0.6"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.0.0-beta.10",
    "@vitejs/plugin-vue": "2.1.0",
    "sass": "1.49.0",
    "sass-loader": "12.4.0",
    "vite": "2.7.13"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/kolibrek/vite-tauri-template"
  },
  "license": "ISC"
}
